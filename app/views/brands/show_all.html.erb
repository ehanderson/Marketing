<td><%= link_to 'New', new_brand_path %></td>
<table id="table-bro">
  <tr id="table-header">
  <% Brand.column_names.each do |col| %>
    <th><%= col %></th>
  <% end %>
  </tr>

  <% if @brands %>
    <tr >
    <% @brands.each do |brand| %>
      <% Brand.column_names.each do |col| %>
        <td><%= brand.attributes.values_at(col) %></td>
      <% end %>
        <td><%= button_to "Delete", brand, :method => :delete %></td>
        <td><%= link_to 'Edit', edit_brand_path(brand) %></td>

    </tr>
    <% end %>
  <% end %>

</table>
